            this.CreatedBy = balanceTO.CreatedBy;
            this.CreatedTime = balanceTO.CreatedTime;
            this.ModifiedBy = balanceTO.ModifiedBy;
            this.ModifiedTime = balanceTO.ModifiedTime;
        }
    }
