        {
            get { return _emplID; }
            set { _emplID = value; }
        }

        public DateTime Month
        {
            get { return _month; }
            set { _month = value; }
        }

        public int ValuePaid
        {
            get { return _valuePaid; }
            set { _valuePaid = value; }
        }

        public int RecalcFlag
        {
            get { return _recalcFlag; }
            set { _recalcFlag = value; }
        }
        
