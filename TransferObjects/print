        {
            this.Approved = meal.Approved;
            this.ApprovedBy = meal.ApprovedBy;
            this.ApprovedDesc = meal.ApprovedDesc;
            this.ApprovedTime = meal.ApprovedTime;
            this.AutoCheck = meal.AutoCheck;
            this.AutoCheckFailureReason = meal.AutoCheckFailureReason;
            this.AutoCheckTime = meal.AutoCheckTime;
            this.CreatedBy = meal.CreatedBy;
            this.CreatedTime = meal.CreatedTime;
            this.EmployeeID = meal.EmployeeID;
            this.EventTime = meal.EventTime;
            this.ManualCreated = meal.ManualCreated;
            this.MealTypeID = meal.MealTypeID;
            this.OnlineValidation = meal.OnlineValidation;
            this.PointID = meal.PointID;
            this.Qty = meal.Qty;
            this.ReasonRefused = meal.ReasonRefused; 
            this.TransactionID = meal.TransactionID;           
        }
    }
