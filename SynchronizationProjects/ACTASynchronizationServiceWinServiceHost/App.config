  <system.serviceModel>
    <services>
      <service name="ACTASynchronizationService.ACTASynchronizationService" behaviorConfiguration="ACTASynchronizationServiceBehavior">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:44457/ACTASynchronizationService" />
          </baseAddresses>
